{
  "source": {
    "include": ["src"],
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|docs|test?s)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "recuse": true,
    "destination": "./docs"
  }
}
